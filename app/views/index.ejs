<% include header %>

  <h1><%-title %></h1>
  <p>Upload your file.</p>

  <form action="/upload" method="post" enctype="multipart/form-data">
  <input type="file" name="snapshot" />
  <input type="submit" value="Upload" />
  </form>

  <% images.forEach(function(image) { %>


  <div id="pictures">
  	<ul>
  		<li><img src="<%= image %>" width="100"/></li>
  	</ul>
  </div>

  <% }); %>

<% include footer %>